<?xml version="1.0" ?>
<assembly>
	<id>win32</id>
	<formats>
		<format>zip</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>
	<dependencySets>
		<dependencySet>
			<outputFileNameMapping>${artifact.artifactId}${dashClassifier?}.${artifact.extension}</outputFileNameMapping>
			<outputDirectory>lib</outputDirectory>
			<excludes>
				<exclude>junit:junit</exclude>
				<exclude>at.uni-salzburg.cs.ckgroup.jnavigator.jnavigator-io-native.linux-x86:libJNavigator</exclude>
			</excludes>
		</dependencySet>
	</dependencySets>
	<fileSets>
		<fileSet>
			<directory>images</directory>
			<outputDirectory>images</outputDirectory>
			<includes>
				<include>**/*.jpg</include>
			</includes>
			<lineEnding>keep</lineEnding>
		</fileSet>
		<fileSet>
			<directory>models</directory>
			<outputDirectory>models</outputDirectory>
			<includes>
				<include>**/*.3ds</include>
			</includes>
			<lineEnding>keep</lineEnding>
		</fileSet>
	</fileSets>
	<files>
		<file>
			<source>config.xml</source>
			<outputDirectory />
			<lineEnding>dos</lineEnding>
		</file>
		<file>
			<source>src/main/scripts/ControlTerminal.bat</source>
			<outputDirectory />
			<lineEnding>dos</lineEnding>
			<filtered>true</filtered>
		</file>
		<file>
			<source>ControlTerminal.cfg</source>
			<outputDirectory />
			<lineEnding>dos</lineEnding>
		</file>
	</files>
</assembly>
